git_url(ENV['APP_CERTIFICATE_GIT_URL'])

storage_mode("git")

type(ENV['MATCH_TYPE']) # The default type, can be: appstore, adhoc, enterprise or development

app_identifier([ENV['APP_IDENTIFIER']])

# Your Apple Developer Portal username
username(ENV['MATCH_USER'])

